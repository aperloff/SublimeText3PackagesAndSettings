TF1 *flog = new TF1("flog", "[0]+((1.0/(TMath::Log10(x)*TMath::Sqrt(2.0*TMath::Pi()*TMath::Power([2],2))))*TMath::Exp(-TMath::Power(TMath::Log(x)-TMath::Log([1]),2)/(2.0*TMath::Power([2],2))))", 3, 30)
